@ARTICLE{Nam202125,
author={Nam, K.-H. and Jung, E.-S. and Park, J. and Yun, S.-K.},
title={Clinicopathological and Dermoscopic Features of Clear Cell Acanthoma [투명 세포 극세포종의 임상, 병리조직학 및 피부확대경소견에 대한 연구]},
journal={Korean Journal of Dermatology},
year={2021},
