@ARTICLE{Papathanasopoulos2007304,
author={Papathanasopoulos, A. and Bali, Ch. and Tsampoulas, K. and Charisis, Ch. and Kappas, A.M.},
title={Imaging of anal cancer with the use of endoanal-endorectal ultrasonography},
